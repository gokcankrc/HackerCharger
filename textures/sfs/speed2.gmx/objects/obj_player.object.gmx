var up_coll = collision_line(x + 64 * sin(direction*pi/180), y + 64 * cos(direction*pi/180), x + 64 * sin(direction*pi/180) + hspeed, y + 64 * cos(direction*pi/180) + vspeed, obj_block,true,true)
var down_coll = collision_line(x - 64 * sin(direction*pi/180), y - 64 * cos(direction*pi/180), x - 64 * sin(direction*pi/180) + hspeed, y - 64 * cos(direction*pi/180) + vspeed, obj_block,true,true) 

 
if up_coll != noone
    {
    var inst = up_coll.object_index
    if object_is_ancestor( inst, obj_death) or object_is_ancestor( inst, obj_blockdeath) then coll_with_death = true
    }
if down_coll != noone
    {
    var inst = down_coll.object_index
    if object_is_ancestor( inst, obj_death) or object_is_ancestor( inst, obj_blockdeath) then coll_with_death = true
    }
if mid_coll != noone
    {
    var inst = mid_coll.object_index
    if object_is_ancestor( inst, obj_death) or object_is_ancestor( inst, obj_blockdeath) then coll_with_death = true
    }


    
if coll_with_death
    {
    instance_create(x,y,obj_player_death)
    
    with(obj_cursor) {instance_destroy()}
    instance_destroy()
    }
    
    
move_bounce_solid(true)



    







//slowing and speeding time------------------------
if mouse_check_button_pressed(mb_left)
    {
    slow = true
    slowtime = 90
    global.all_var = global.all_var_low
    speed *= global.all_var
    
    instance_create(x,y,obj_cursor)
    
    }
if mouse_check_button_released(mb_left) or slowtime = 0
    {
    slow = false
    slowtime = -1
    speed /= global.all_var
    global.all_var = 1
    speed += acc
    direction = point_direction (x,y, mouse_x, mouse_y)
    
    instance_destroy(obj_cursor)
    
    }

    
image_speed = speed / 200
if slow
    {
    slowtime--
    image_angle = point_direction(x,y,mouse_x, mouse_y)
    image_speed = global.all_var * speed / 288
    }
else image_angle = direction



//hovering sprite effect---------------------------------

hover_cd -= 1 * global.all_var;
if hover_cd &lt;= 0
